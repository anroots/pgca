{
  "name": "anroots/pgca",
  "description": "Analyzes Git commit messages",
  "keywords": [
    "pgca"
  ],
  "homepage": "https://github.com/anroots/pgca",
  "license": "MIT",
  "authors": [
    {
      "name": "Ando Roots",
      "email": "ando@sqroot.eu",
      "homepage": "http://sqroot.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "symfony/console": "~2.6",
    "symfony/dependency-injection": "~2.6",
    "symfony/config": "~2.6",
    "dflydev/dot-access-data": "dev-master",
    "gitonomy/gitlib": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "phpmd/phpmd": "~2.2",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Anroots\\Pgca\\": "src/Anroots/Pgca"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Anroots\\Pgca\\Test\\": "tests"
    }
  },
  "bin": ["bin/pgca"],
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
